{
  "functions": {
    "api/*.go": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/:path*",
      "dest": "/api/index.go"
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/api/index/:path*"
    }
  ]
}
